generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

generator prismabox { 
  provider = "prismabox"
  typeboxImportDependencyName = "elysia"
  typeboxImportVariableName = "t"
  inputModel = true
} 

model User {
  id    String  @id @default(cuid())
  email String  @unique
  name  String?
  password String
}

